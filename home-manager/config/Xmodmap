! French letters on external keyboard using F13+ keys, see:
! https://github.com/tarneaux/zmk-config-triboard/blob/main/config/triboard.keymap
! é
keycode 191 = eacute Eacute
! è
keycode 192 = egrave Egrave
! à
keycode 193 = agrave Agrave
! circunflex and diaeresis/umlaut for use on next keypress (ê, ë, î, ï, ô, ö, û, ü)
keycode 194 = dead_circumflex dead_diaeresis
! ç
keycode 195 = ccedilla Ccedilla
! ù
keycode 196 = ugrave Ugrave
! · (median point)
keycode 197 = periodcentered periodcentered
